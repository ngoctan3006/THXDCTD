PROGRAM EXAMPLE9;
VAR N : INTEGER;

PROCEDURE INPUT;
BEGIN
    N := READI;
END;

FUNCTION FINOBACCI(K : INTEGER) : INTEGER;
BEGIN
    IF K = 1 THEN FINOBACCI := 1;
    IF K = 2 THEN FINOBACCI := 1;
    IF K > 2 THEN FINOBACCI := FINOBACCI(K - 1) +  FINOBACCI(K-2);
END;   

BEGIN
    CALL INPUT;
    CALL WRITEI(FINOBACCI(N));
END.

